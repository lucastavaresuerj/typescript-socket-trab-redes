{
  "devDependencies": {
    "@types/node": "^17.0.31",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "start": "npx ts-node src/index.ts",
    "start:server": "npm start -- --type=server --port=3000",
    "start:client": "npm start -- --type=client --port=3000",
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'npm start'"
  },
  "dependencies": {}
}
